<script type="text/javascript">
$(document).ready(function(){
	$("#search_books_form").on("ajax:success", function(e, data, status, xhr){
		if(status != 'success')
			$("#content").html("<div class=\"row\"><h3>Non ho trovato niente</h3></div>");
		else
			$("#content").html(data);
	});
});
